{
  "name": "ytrocket",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "types": "tsc --noEmit",
    "check": "pnpm types && pnpm lint && pnpm test",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint:commit": "eslint --max-warnings 0 --fix"
  },
  "dependencies": {
    "husky": "^9.1.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "husky": "^9.0.11",
    "jest": "^29.2.1",
    "prettier": "^2.4.1",
    "lint-staged": "^15.2.7",
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1"
  },
  "lint-staged": {
    "**/*.ts": "tsc-files --noEmit",
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{js,ts,tsx,css}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
